alias ll="ls -F -l"
alias lh="ls -d .!(|.)"
alias e="emacs"
alias em="emacs"
alias et="emacs -nw"
alias xe="xelatex"
function jr {
    mthyr=`date +"%B %Y"`
    if [ ! -f ~/Dropbox/write/"$mthyr".txt ]
    then
	emacs -nw ~/Dropbox/write/"$mthyr".txt --eval "(progn (today) (newline) (time) (newline) (newline))"
    else
	emacs -nw ~/Dropbox/write/"$mthyr".txt --eval "(progn (goto-char (point-max)) (newline) (today) (newline) (time) (newline) (newline))"
    fi
}
