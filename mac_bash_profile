PS1="\u \w $ "
function quad {
    if [ -z "$@"]; then
	cd ~/Code/Scriptures/
    else
	for var in "$@"
	do
	    grep -ri --color=auto "$var" "/Users/mike/Code/Scriptures"
	done
    fi
}
